		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ЗаказПоставщикуТовары.Ссылка.Партнер КАК Партнер,
	ЗаказПоставщикуТовары.Ссылка КАК Ссылка,
	ЗаказПоставщикуТовары.Ссылка.Дата КАК Дата,
	ЗаказПоставщикуТовары.Ссылка.НомерПоДаннымПоставщика КАК НомерПоДаннымПоставщика,
	ЗаказПоставщикуТовары.Ссылка.ДатаПоДаннымПоставщика КАК ДатаПоДаннымПоставщика,
	ЗаказПоставщикуТовары.Номенклатура КАК Номенклатура,
	ЗаказПоставщикуТовары.Количество КАК Количество,
	ПоступлениеДо.Количество КАК ОбъемПродукцииНаСкладе,
	ПоступлениеОтИДо.Количество КАК ОбъемПоступленийНаСкладЗаПериод,
	ЗаказПоставщикуТовары.Количество - ПоступлениеДо.Количество КАК НеЗашло,
	ЗаказПоставщикуТовары.Цена КАК Цена,
	ЗаказПоставщикуТовары.СуммаСНДС КАК СуммаСНДС,
	ЗаказПоставщикуТовары.Ссылка.Менеджер КАК Менеджер,
	ЗаказПоставщикуТовары.Ссылка.ТипПеревозки КАК ТипПеревозки,
	ВЫБОР
		КОГДА ЗаказПоставщикуТовары.Ссылка.СпособДоставки = ЗНАЧЕНИЕ(Перечисление.СпособыДоставки.СиламиПоставщикаДоНашегоСклада)
			ТОГДА "CPT"
		ИНАЧЕ "EXW"
	КОНЕЦ КАК Базис,
	ЕСТЬNULL(ЗаказПоставщикуТовары.Цена, 0) / ((100 + ВЫБОР
		КОГДА ЗаказПоставщикуТовары.СтавкаНДС = ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС10)
			ТОГДА 10
		КОГДА ЗаказПоставщикуТовары.СтавкаНДС = ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС18)
			ТОГДА 18
		КОГДА ЗаказПоставщикуТовары.СтавкаНДС = ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС20)
			ТОГДА 20
		ИНАЧЕ 0
	КОНЕЦ) / 100) КАК ЦенаТовараЗаТоннуБезНДС,
	ЗаказПоставщикуТовары.СтавкаНДС КАК СтавкаНДС_Товар,
	ОтгрузкаОтПоставщикаЦена.ЦенаПеревозкиНаТонну_с_НДС КАК ЦенаПеревозкиНаТонну_с_НДС,
	ВЫБОР
		КОГДА ОтгрузкаОтПоставщикаКолСтавкаНДС.КоличествоРазличных &gt; 1
			ТОГДА "Разная"
		ИНАЧЕ ОтгрузкаОтПоставщикаСтавкаНДС.СтавкаНДС
	КОНЕЦ КАК СтавкаНДС_Перевозка,
	ОтгрузкаОтПоставщикаЦена.ЦенаПеревозкиНаТонну_без_НДС КАК ЦенаПеревозкиНаТонну_без_НДС,
	ЕСТЬNULL(ЗаказПоставщикуТовары.Цена, 0) / ((100 + ВЫБОР
		КОГДА ЗаказПоставщикуТовары.СтавкаНДС = ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС10)
			ТОГДА 10
		КОГДА ЗаказПоставщикуТовары.СтавкаНДС = ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС18)
			ТОГДА 18
		КОГДА ЗаказПоставщикуТовары.СтавкаНДС = ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС20)
			ТОГДА 20
		ИНАЧЕ 0
	КОНЕЦ) / 100) + ЕСТЬNULL(ОтгрузкаОтПоставщикаЦена.ЦенаПеревозкиНаТонну_без_НДС, 0) КАК ЦенаCPT_ОбщаяБез_НДС,
	ЕСТЬNULL(ЗаказПоставщикуТовары.Цена, 0) + ЕСТЬNULL(ОтгрузкаОтПоставщикаЦена.ЦенаПеревозкиНаТонну_с_НДС, 0) КАК ЦенаCPT_ОбщаяС_НДС,
	(ЕСТЬNULL(ЗаказПоставщикуТовары.Цена, 0) / ((100 + ВЫБОР
		КОГДА ЗаказПоставщикуТовары.СтавкаНДС = ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС10)
			ТОГДА 10
		КОГДА ЗаказПоставщикуТовары.СтавкаНДС = ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС18)
