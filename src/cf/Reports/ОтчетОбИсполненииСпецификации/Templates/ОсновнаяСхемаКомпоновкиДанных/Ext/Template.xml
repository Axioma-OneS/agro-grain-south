		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ЗаказПоставщикуТовары.Ссылка КАК Ссылка,
	ЗаказПоставщикуТовары.Ссылка.Дата КАК Дата,
	ЗаказПоставщикуТовары.Ссылка.Партнер КАК Поставщик,
	ЗаказПоставщикуТовары.Ссылка.Договор КАК Договор,
	ВЫБОР
		КОГДА ЗаказПоставщикуТовары.Ссылка.СпособДоставки = ЗНАЧЕНИЕ(Перечисление.СпособыДоставки.СиламиПоставщикаДоНашегоСклада)
			ТОГДА "CPT"
		ИНАЧЕ "EXW"
	КОНЕЦ КАК БазисПоставки,
	ЗаказПоставщикуТовары.Номенклатура КАК Номенклатура,
	ЗаказПоставщикуТовары.Цена КАК ЦенаТовара,
	ВЫБОР
		КОГДА ЗаказПоставщикуТовары.СтавкаНДС = ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС10)
			ТОГДА ЗаказПоставщикуТовары.Цена * ((100 + 10) / 100)
		КОГДА ЗаказПоставщикуТовары.СтавкаНДС = ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС18)
			ТОГДА ЗаказПоставщикуТовары.Цена * ((100 + 18) / 100)
		КОГДА ЗаказПоставщикуТовары.СтавкаНДС = ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС20)
			ТОГДА ЗаказПоставщикуТовары.Цена * ((100 + 20) / 100)
		ИНАЧЕ ЗаказПоставщикуТовары.Цена * ((100 + 0) / 100)
	КОНЕЦ КАК ЦенаТовараСНДС,
	взСредняяЦенаПеревозки.Значение КАК СредняяЦенаПеревозки,
	ВЫБОР
		КОГДА ЗаказПоставщикуТовары.СтавкаНДС = ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС10)
			ТОГДА взСредняяЦенаПеревозки.Значение / ((100 + 10) / 100)
		КОГДА ЗаказПоставщикуТовары.СтавкаНДС = ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС18)
			ТОГДА взСредняяЦенаПеревозки.Значение / ((100 + 18) / 100)
		КОГДА ЗаказПоставщикуТовары.СтавкаНДС = ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС20)
			ТОГДА взСредняяЦенаПеревозки.Значение / ((100 + 20) / 100)
		ИНАЧЕ взСредняяЦенаПеревозки.Значение / ((100 + 0) / 100)
	КОНЕЦ КАК СредняяЦенаПеревозкиБезНДС,
	ЗаказПоставщикуТовары.Цена + ЕСТЬNULL(взСредняяЦенаПеревозки.Значение, 0) КАК ЦенаТовара_CPT,
	ВЫБОР
		КОГДА ЗаказПоставщикуТовары.СтавкаНДС = ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС10)
			ТОГДА (ЗаказПоставщикуТовары.Цена + ЕСТЬNULL(взСредняяЦенаПеревозки.Значение, 0)) * ((100 + 10) / 100)
		КОГДА ЗаказПоставщикуТовары.СтавкаНДС = ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС18)
			ТОГДА (ЗаказПоставщикуТовары.Цена + ЕСТЬNULL(взСредняяЦенаПеревозки.Значение, 0)) * ((100 + 18) / 100)
		КОГДА ЗаказПоставщикуТовары.СтавкаНДС = ЗНАЧЕНИЕ(Перечисление.СтавкиНДС.НДС20)
			ТОГДА (ЗаказПоставщикуТовары.Цена + ЕСТЬNULL(взСредняяЦенаПеревозки.Значение, 0)) * ((100 + 20) / 100)
		ИНАЧЕ (ЗаказПоставщикуТовары.Цена + ЕСТЬNULL(взСредняяЦенаПеревозки.Значение, 0)) * ((100 + 0) / 100)
	КОНЕЦ КАК ЦенаТовара_CPTСНДС,
	ЗаказПоставщикуТовары.Количество КАК ОбщийОбъем,
	ЗаказПоставщикуТовары.Ссылка.ДатаОкончанияСпецификации КАК ДатаОкончанияСпецификации,
	ЗаказПоставщикуТовары.Количество - ЕСТЬNULL(взСуммаВесов.Вес, 0) - ЕСТЬNULL(взПоступленияДоставкаПоставщикаДа.Количество, 0) КАК ОстатокУПоставщика,
	ЕСТЬNULL(взСуммаВесов.Вес, 0) + ЕСТЬNULL(взПоступленияДоставкаПоставщикаДа.Количество, 0) КАК ФактОтгрузки,
	ЗаказПоставщикуТовары.Ссылка.ПлановыйСрокЗавершенияОтгрузок КАК ПлановыйСрокЗавершенияОтгрузок_для_EXW,
	взСуммаВесовВПути.Вес КАК ОбъемВПути,
	взПоступленияВсе.Количество КАК ФактПоступления,
